{
    "routes": [
        {
            "route": "/images/*",
            "serve": "/images/:path*"
        },
        {
            "route": "/videos/*",
            "serve": "/videos/:path*"
        },
        {
            "route": "/favicon.ico",
            "serve": "/favicon.ico"
        },
        {
            "route": "/*",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/videos/*",
            "/favicon.ico",
            "/*.{png,jpg,jpeg,gif,webp,svg}"
        ]
    },
    "mimeTypes": {
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".jpeg": "image/jpeg",
        ".gif": "image/gif",
        ".webp": "image/webp",
        ".svg": "image/svg+xml",
        ".mp4": "video/mp4",
        ".mov": "video/quicktime",
        ".webm": "video/webm"
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    }
}