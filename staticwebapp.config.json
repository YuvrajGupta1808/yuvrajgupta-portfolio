{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/*.{png,jpg,jpeg,gif,webp,svg}",
            "/favicon.ico",
            "/assets/*",
            "/videos/*"
        ]
    },
    "mimeTypes": {
        ".png": "image/png",
        ".jpg": "image/jpeg",
        ".jpeg": "image/jpeg",
        ".gif": "image/gif",
        ".webp": "image/webp",
        ".svg": "image/svg+xml",
        ".mp4": "video/mp4",
        ".mov": "video/quicktime",
        ".webm": "video/webm"
    }
}